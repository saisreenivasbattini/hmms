<%= form_for(@mm_range) do |f| %>
  <% if @mm_range.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@mm_range.errors.count, "error") %> prohibited this mm_range from being saved:</h2>

      <ul>
      <% @mm_range.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :low %><br>
    <%= f.text_field :low %>
  </div>
  <div class="field">
    <%= f.label :high %><br>
    <%= f.text_field :high %>
  </div>
  <div class="field">
    <%= f.label :state_id %><br>
    <%= f.select :state_id, options_for_select(State.all.map { |state| [state.name, state.id] })%>
  </div>
  <div class="field">
    <%= f.label :region_id %><br>
    <%= f.select :region_id, options_for_select(Region.all.map { |region| [region.name, region.id] })%>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

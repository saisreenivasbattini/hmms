 </div>




<script type="text/javascript">
  $(document).ready(function(){
    $("#tab1").addClass("active");
    $("#tab1").click(function () {
    $("#tab1").addClass("active");
    $("#tab2").removeClass("active");

    $(".tab1_content").show(500);
      $(".tab2_content").hide(500);

   });


  $("#tab2").click(function () {
    $("#tab2").addClass("active");
      $("#tab1").removeClass("active");

      $(".tab1_content").hide("slow");
    $(".tab2_content").show("slow");

   });

$("#register.btn").off('click').on('click', function(){
  console.log("register");
    $(".register_form").show();
    $("#register.btn").addClass("active");
  $("#close").show();
});

  $("#close").off('click').on('click',function(){
  console.log("close");
    $(".register_form").hide();
  $("#close").hide();
  $("#register.btn").removeClass("active");
  });

   $(".signup").off('click').on('click',function(){
  console.log("signup");
    $(".form2").show();
  $("#close").hide();
   $(".register_form").hide();
    $("#register.btn").addClass("active");
  $("#close1").show();
  });

  $("#close1").off('click').on('click',function(){
  console.log("close1");
    $("#form2").hide();
  $("#close1").hide();
   $("#register").removeClass("active");

  });
  });

</script>
